{
    "name": "vega-multi-view",
    "version": "1.0.0",
    "description": "add multiple separate vega views to a page and connect the signals of these views",
    "license": "MIT",
    "author": "abudaan",
    "main": "dist/index.js",
    "module": "src/js/index.js",
    "repository": "git://github.com/abudaan/vega-multi-view.git",
    "bugs": {
        "url": "https://github.com/abudaan/vega-multi-view/issues"
    },
    "scripts": {
        "test": "watchify ./test/js/index.js -t [ babelify ] -o ./test/app.js --debug --verbose",
        "watch": "watchify ./src/js/index.js -t [ babelify ] -o ./build/vega-multi-view.js --debug --verbose",
        "build-css": "node-sass ./src/styles/main.sass > ./dist/vega-multi-view.css",
        "build": "rollup -c",
        "compile": "babel src/js --out-dir ./dist",
        "compile:production": "NODE_ENV=production babel src/js --out-dir ./dist",
        "server": "python -m SimpleHTTPServer 9001",
        "copy-dist": "cp -R ../vega-multi-view/dist ../fffact/node_modules/vega-multi-view/",
        "copy-src": "cp -R ../vega-multi-view/src ../fffact/node_modules/vega-multi-view/",
        "copy-pkg": "cp ../vega-multi-view/package.json ../fffact/node_modules/vega-multi-view/",
        "copy": "npm run copy-dist && npm run copy-src && npm run copy-pkg",
        "update": "npm run compile && npm run copy"
    },
    "dependencies": {
        "cson-parser": "^2.0.0",
        "leaflet": "^1.2.0",
        "ramda": "0.23.0",
        "vega": "^3.0.2",
        "vega-tooltip": "^0.4.3",
        "xstream": "^10.9.0",
        "yamljs": "^0.3.0"
    },
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-eslint": "^7.2.3",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-preset-env": "^1.6.0",
        "babel-preset-flow": "^6.23.0",
        "babel-preset-stage-0": "^6.24.1",
        "babelify": "^7.3.0",
        "eslint": "3.19.0",
        "eslint-config-airbnb": "14.1.0",
        "eslint-plugin-babel": "4.1.1",
        "eslint-plugin-flowtype": "2.30.4",
        "eslint-plugin-import": "2.2.0",
        "eslint-plugin-jsx-a11y": "4.0.0",
        "eslint-plugin-react": "6.10.0",
        "flow-bin": "0.42.0",
        "sort-package-json": "^1.7.1",
        "watchify": "^3.9.0"
    }
}
