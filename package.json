{
    "name": "vega-multi-view",
    "version": "1.0.0",
    "description": "add multiple separate vega views to a page and connect the signals of these views",
    "license": "MIT",
    "author": "abudaan",
    "main": "dist/index.js",
    "module": "src/js/index.js",
    "scripts": {
        "test": "watchify ./test/js/index.js -t [ babelify ] -o ./test/app.js --debug --verbose",
        "watch": "watchify ./src/js/index.js -t [ babelify ] -o ./build/vega-multi-view.js --debug --verbose",
        "build-css": "node-sass ./src/styles/main.sass > ./dist/vega-multi-view.css",
        "build": "rollup -c",
        "compile": "babel src/js --out-dir ./dist",
        "server": "python -m SimpleHTTPServer 9001",
        "copy-dist": "cp -R ../vega-multi-view/dist ../fffact/node_modules/vega-multi-view/",
        "copy-src": "cp -R ../vega-multi-view/src ../fffact/node_modules/vega-multi-view/",
        "copy": "npm run copy-dist && npm run copy-src",
        "update": "npm run compile && npm run copy"
    },
    "dependencies": {
        "cson-parser": "^2.0.0",
        "leaflet": "^1.2.0",
        "ramda": "0.23.0",
        "rxjs": "^5.4.3",
        "vega": "^3.0.2",
        "vega-tooltip": "^0.4.3",
        "xstream": "^10.9.0",
        "yamljs": "^0.3.0"
    },
    "devDependencies": {
        "gulp-rollup": "^2.14.0",
        "babel-cli": "^6.26.0",
        "babel-eslint": "^7.2.3",
        "babel-loader": "^7.1.2",
        "babel-plugin-external-helpers": "^6.22.0",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-preset-babili": "^0.1.4",
        "babel-preset-env": "^1.6.0",
        "babel-preset-flow": "^6.23.0",
        "babel-preset-latest": "^6.24.1",
        "babelify": "^7.3.0",
        "browserify": "^14.4.0",
        "eslint": "3.19.0",
        "eslint-config-airbnb": "14.1.0",
        "eslint-plugin-babel": "4.1.1",
        "eslint-plugin-flowtype": "2.30.4",
        "eslint-plugin-import": "2.2.0",
        "eslint-plugin-jsx-a11y": "4.0.0",
        "eslint-plugin-react": "6.10.0",
        "flow-bin": "0.42.0",
        "gulp": "^3.9.1",
        "gulp-autoprefixer": "^4.0.0",
        "gulp-babili": "^0.1.4",
        "gulp-concat": "^2.6.1",
        "gulp-rename": "^1.2.2",
        "gulp-sass": "^3.1.0",
        "gulp-sourcemaps": "^2.6.1",
        "gulp-util": "^3.0.8",
        "json-loader": "^0.5.7",
        "rollup": "^0.49.2",
        "rollup-plugin-babel": "^3.0.2",
        "rollup-plugin-commonjs": "^8.2.0",
        "rollup-plugin-json": "^2.3.0",
        "rollup-plugin-node-resolve": "^3.0.0",
        "sort-package-json": "^1.7.1",
        "vinyl-buffer": "^1.0.0",
        "vinyl-source-stream": "^1.1.0",
        "watchify": "^3.9.0",
        "webpack": "^3.5.5"
    }
}
